				"description": "Sample ID used by the lab (if not the same as NPI_sample_id)",
				"type": "string"
		},
		 "biomarker_category": {
				 "description": "Category of biomarker",
				 "type": "string",
				 "enum": [
					 "hormones",
					 "metabolites",
					 "oxidative stress",
					 "vitamins"
					 ]
		 },
		 "biomarker": {
				 "description": "Type of biomarker",
				 "type": "string",
				 "enum": [
					 "cholesterol",
					 "triglycerides",
					 "EROD",
					 "retinol",
					 "retinyl_palmitate",
					 "testosterone_6B",
					 "uro_porphyrin",
					 "hepta_porphyrin",
					 "hexa_porphyrin",
					 "penta_porphyrin",
					 "copro_porphyrin",
					 "proto_porphyrin"
				 ]
			 },
			 "efficiency": {
					 "description": "Efficiency for gene expressions",
					 "type": ["string", "null"]
			 },
			 "unit": {
					 "description": "Unit of biomarker value",
					 "type": ["string", "null"]
			 },
			 "percent_recovery": {
	 				"description": "Percentage recovered of a spiked reference sample",
	 				"type": ["number", "null"]
	 		 },
			 "detection_limit": {
					 "description": "Smallest concentration detectable (LOD)",
					 "type": ["number", "null"]
			 },
			 "level_of_quantification": {
					 "description": "The amount measured above this limit is more accurate (LOQ)",
					 "type": ["number", "null"]
			 },
			 "comment": {
					 "description": "Sample comment",
					 "type": ["string","null"]
			 },
		   "sample_values": {
						"title": "Measured value for biomarker",
						"type": "array",
						"additionalItems": false,
						"uniqueItems": false,
						"items": {
								"type": "string"
			 }
			}
		},
			"links": {
            "type": "array",
            "additionalItems": false,
            "items": {
              "type": "object",
              "additionalProperties": false,
              "properties": {
                "rel": {
                  "type": "string",
                  "enum": [
                    "data"
                  ]
                },
                "href": {
									"description": "The actual link fieldwork data,likely to ecotox-fieldwork or marine-biology-sample",
                  "type": "string",
                  "format": "uri"
                },
                "title": {
									"description": "Title of linked resource",
                  "type": "string",
									"enum": [
										"collected sample"
									]
                },
                "hreflang": {
									"description": "Language of the linked resource",
                  "type": "string",
									"enum": [
										"collected sample"
									]
                },
                "type": {
									"description": "Mime-type of linked resource",
                  "type": "string"
                }
              }
            }
          },
					"files": {
				      "type": "array",
				      "uniqueItems": true,
				      "description": "",
				      "items": {
				        "type": "object",
				        "properties": {
										"uri": {
											"description": "Uri to excel form",
											"type": "string",
											"format": "uri"
										},
										"filename": {
											"description": "Excel filename",
											"type": "string"
										},
										"title": {
					            "type": "string",
					            "description": "Title"
					          },
										"type": {
											"description": "Excel type",
											"type": "string"
										},
										"length": {
											"description": "Excel file length",
											"type": "integer"
										},
										"hash": {
							 				"type": "string",
							 			  "description": "MD5 hash of file name"
						 			 }
						}
			}
