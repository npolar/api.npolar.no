{
   "id": "radiation-zeppelin-api",
   "schema": "http://api.npolar.no/schema/api",
   "path": "/radiation/zeppelin",
   "title": "Incoming radiation at Zeppelin Station",
   "summary": "",
   "storage": "Couch",
   "database": "radiation-zeppelin",
   "search": {
       "engine": "Elasticsearch",
       "uri": "http://localhost:9200",
       "facets": [
         "t2_last_sampling"
       ],
       "type": "radiation-zeppelin",
       "index": "radiation-zeppelin"
   },
   "base": "http://api.npolar.no",
   "verbs": [
       "DELETE",
       "GET",
       "HEAD",
       "POST",
       "PUT"
   ],
   "formats": {
       "json": "application/json",
       "html": "text/html"
   },
   "accepts": {
       "application/json": "",
       "text/html": "",
       "text/plain": "",
       "text/tab-separated-values": ""
   },
   "category": [
       "CouchDB",
       "REST",
       "JSON",
       "HTTP"
   ],
   "lifecycle": "production",
   "run": "Npolar::Api::Json",
   "middleware": [
       [
           "Npolar::Rack::ZeppelinTsvParser",
           {
           }
       ]
   ],
   "open": true,
   "auth": {
       "authorize": "editor",
       "system": "zeppelin",
       "authorizer": "Npolar::Auth::Ldap"
   }
}
