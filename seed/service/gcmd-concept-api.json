{
    "_id": "gcmd-concept-api",
    "schema": "http://api.npolar.no/schema/api",
    "id": "gcmd-concept-api",
    "path": "/gcmd/concept",
    "title": "GCMD Concept",
    "summary": "Global Change Master Directory concepts",
    "storage": "Couch",
    "database": "gcmd_concept",
    "search": {
        "engine": "Elasticsearch",
        "index": "gcmd",
        "type": "concept",
        "params": {
            "facets": "concept,cardinality,version,ancestors,label"
        }
    },
    "verbs": [
        "DELETE",
        "GET",
        "HEAD",
        "POST",
        "PUT",
        "OPTIONS"
    ],
    "formats": {
        "json": "application/json"
    },
    "category": [
        "CouchDB",
        "REST",
        "JSON",
        "HTTP"
    ],
    "lifecycle": "production",
    "run": "Npolar::Api::Json",
    "open": true,
    "base": "http://api.npolar.no",
    "accepts": {
        "application/json": "JSON"
    },
    "middleware": [
        [
            "::Rack::Gouncer",
            {
                "url": "https://api.npolar.no:8950",
                "open": true,
                "system": "https://api.npolar.no/admin"
            }
        ]
    ]
}
