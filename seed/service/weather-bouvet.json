   ],
   "formats": {
       "json": "application/json",
       "html": "text/html"
   },
   "accepts": {
       "text/csv": ""
   },
   "category": [
       "CouchDB",
       "REST",
       "JSON",
       "HTTP"
   ],
   "lifecycle": "production",
   "run": "Npolar::Api::Json",
