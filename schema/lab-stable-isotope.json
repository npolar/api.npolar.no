{
	"id": "http://api.npolar.no/schema/lab-ecotox",
	"$schema": "http://json-schema.org/draft-04/schema",
	"title": "Ecotox lab results",
	"description": "Norwegian Polar Institute's ecotox lab results database.",
	"type": "object",
	"required": [
		"id",
		"schema"
	],
	"properties": {
		"id": {
			"description": "Resource identifier (UUID).",
			"type": "string"
		},
		"schema": {
			"description": "Schema defining this object (this schema).",
			"type": "string",
			"enum": [
				"http://api.npolar.no/schema/lab-ecotox"
			]
		},
		"lang": {
			"description": "The language of the document",
			"type": "string",
			"default": "en",
			"pattern": "^[a-z]{2}$"
		},
		"laboratory": {
			"description": "Name of laboratory performing analysis",
			"enum": ["IFE"],
			"type": "string"
		},
		"lab_report_id": {
			"description": "Name/number of report given by the lab",
			"type": "string"
		},
		"ref_no": {
			"description": "Reference number for one set of analyses",
			"type": "string"
		},
		"date_report": {
			"description": "Date of the report",
			"type": "string",
			"format": "date-time"
		},
		"excel": {
			"description": "Original excel file(s)",
			"type": "array",
			"additionalItems": false,
			"items": {
				"type": "object",
				"additionalProperties": false,
				"properties": {
					"excel_uri": {
						"description": "Uri to excel form",
						"type": "string",
						"format": "uri"
					},
					"excel_filename": {
						"description": "Excel filename",
						"type": "string"
					},
					"excel_type": {
						"description": "Excel type",
						"type": "string"
					},
					"excel_length": {
						"description": "Excel file length",
						"type": "integer"
					}
				}
			}
		},
		"entry": {
			"title": "List of all entries corresponding to excel rows",
			"type": "array",
			"items": {
				"matrix": {
					"description": "matrix or tissue analysed by the lab",
					"enum": ["whole animal"],
					"type": "string"
				},
				"taxon": {
					"description": "To be used if species is not specified",
					"type": "string"
				},
				"species": {
					"description": "Species",
					"type": "string"
				},
				"sex": {
					"description": "Sex",
					"type": "string"
				},
				"stage": {
					"description": "Stage",
					"type": "string"
				},
				"tissue": {
					"description": "Tissue",
					"type": "string"
				},
				"length": {
					"description": "Length in cm",
					"type": "string"
				},
				"no_of_individuals": {
					"description": "Number of individuals per sample",
					"type": "string"
				},
				"weight": {
					"description": "weight in gram",
					"type": "string"
				},
				"NPI_sample_id": {
					"description": "Sample ID used by NPI",
					"type": "string"
				},
				"lab_sample_id": {
					"description": "Running number for each sample used at the lab",
					"type": "string"
				},
				"NP_field_database": {
					"description": "NP database sample information fetched from.",
					"enum": ["ecotox-fieldwork", "marine-biology-sample"],
					"type": "string"
				},
				"NP_field_database_id": {
					"description": "Sample ID in NP's field database",
					"type": "string",
					"format": "uri"
				},
				"d13Cpdb_EA": {
					"description": "delta13Carbon isotope. The ratio of the naturally occuring carbon isotopes 13Carbon/12Carbon (d13C=13C/12C).pdb-EA refers to he internal standard, Pee Dee Belemnite for ?13C (PDB: USGS 24)",
					"type": "string"
				},
				"d15Nair_EA": {
					"description": "delta15Nitrogen isotope. The ratio of the naturally occuring Nitrogen isotope 15Nitrogen/14Nitrogen (d15N/14N) air",
					"type": ["string", "null"]
				},
				"W_percent_C": {
					"description": "Weight % Carbon",
					"type": ["string", "null"]
				},
				"W_percent_N": {
					"description": "Weight % Nitrogen",
					"type": ["string", "null"]
				},
				"C_ratio_N": {
					"description": "weight ratio of Carbon/Nitrogen wt/wt",
					"type": ["string", "null"]
				},
				"comment": {
					"description": "A free text comment for anything of importance to the batch samples.",
					"type": ["string", "null"]
				}
			}
		},
		"collection": {
			"description": "The storage collection the document belongs to.",
			"type": "string",
			"enum": [
				"ecotox"
			]
		},
		"created": {
			"description": "The date the document was created.",
			"type": "string",
			"format": "date-time"
		},
		"updated": {
			"description": "The date the document was last updated.",
			"type": "string",
			"format": "date-time"
		},
		"created_by": {
			"description": "The username of the person who first created the document in the system.",
			"type": "string"
		},
		"updated_by": {
			"description": "The username of the person who last updated the document in the system.",
			"type": "string"
		},
		"_id": {
			"description": "Internal resource identifier.",
			"type": "string"
		},
		"_rev": {
			"description": "Internal revision number.",
			"type": "string"
		},
		"_deleted": {
			"description": "Deleted entry",
			"type": "string"
		},
		"@context": {
			"NP_id": "https://api.npolar.no/lab-stable-isotop",
			"NPI_field_database_id": {
				"@id": "https://api-test.data.npolar.no/ecotox-fieldwork/?q=&filter-NPI_sample_id="
			}
		}
	}
}
