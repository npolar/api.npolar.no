{{{ head }}}

<article>
<section>{{summary}} workspace on <a href="../">api.npolar.no</a></section>

{{# collections }}
<hr/>
<section>
<h3>{{title}}</h3>

{{summary}}
</section>

<section>
<h4>Sets</h4>

{{# list_formats }}
{{ title }}: <code><a href="{{href}}/.{{format}}">{{href}}/.{{format}}</a></code>
{{/ list_formats }}
{{# sets }}
<code><a href="{{href}}/.{{format}}">{{ title }}</a></code>
{{/ sets }}

</ul>
</section>

<section>
<h4>Document API</h4>

<code>DELETE|GET|HEAD|PUT https://api.npolar.no<a href="{{example_href}}/.json">{{ href }}/id.format</a></code>
<code>POST https://api.npolar.no<a href="{{href}}.{{format}}">{{ href }}</a></code>

<dl>
<dt>Formats (GET, HEAD, DELETE)</dt>
<dd>{{# formats }}
  <code><a href="{{example_href}}.{{format}}">{{ format }}</a></code>
{{/ formats }}
</dd>
<dt>Accepts (POST, PUT)</dt>
<dd>{{# accepts }}
  <code>{{ accept }}</code> <code><a href="{{schema_uri}}">{{schema_uri}}</a></code>
{{/ accepts }}
</dd>

<!--
<dt>Example</dt>
<dd><a href="{{document_example}}">{{document_example}}</a><dd>
</dl>
-->

</section>



<section>
<!--<h4>Search API</h4>
Attributes for filters/facets
-->
</section>

{{/ collections}}


<hr/>
{{# oai }}
<section>
<h3>OAI-PMH repository</h3>

<p>{{ summary }}</p>
<h4>verbs</h4>

<ol>
{{# verbs }}
<li><code><a href="/metadata/oai?verb={{verb}}{{example}}">{{verb}}</a></code></li>
{{/verbs}}
</ol>

<h4>Documentation</h4>
<a href="http://www.openarchives.org/OAI/openarchivesprotocol.html">OAI-PMH 2.0 documentation</a>
</section>
{{/oai}}

</article>
