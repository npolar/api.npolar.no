<article>

<section id="search">
<form class="form-search"><input type="search" autocomplete="autocomplete" id="search_input" name="q" value="" placeholder="{{title}}"
class="yui3-aclist-input input-medium" /><input type="submit" class="btn" value="Search"/>
</form>
</section>

