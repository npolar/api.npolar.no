<section id="summary">{{summary}}

<abbr title="List identifiers">List</abbr>: <code><a href="{{href}}/.{{format}}?limit=10" title="{{title}} (ids)">{{href}}/.{{format}}</a></code>

<abbr title="Outgoing formats for GET requests">Formats</abbr>:
{{# formats }}<code><a href="{{example_id}}.{{format}}" title="Example {{format}} document">{{ format }}</a></code>
{{/ formats }}

<abbr title="Accepts these incxoming formats on POST or PUT requests">Accepts</abbr>:
{{# accepts }}
  <code>{{ accept }}</code> <code><a href="{{schema}}">{{schema_title}}</a></code>
{{/ accepts }}

<abbr title="HTTP methods">Methods</abbr>:
{{# verbs }}
  <code>{{ verb }}</code>
{{/ verbs }}


</section>
</article>